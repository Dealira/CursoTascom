algoritmo "AULA 1"
// Função :
// Autor :
// Data : 21/08/2021
// Seção de Declarações 
var
   ano,nasc,idade:inteiro
inicio

      escreva("Em que anos estamos?")
      leia(ano)
      escreva("Em que ano você nasceu?")
      leia(nasc)
      idade <- ano - nasc
      escreval("Em", ano, " voce tera ", idade, "anos.")


fimalgoritmo
